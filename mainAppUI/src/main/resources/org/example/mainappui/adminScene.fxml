            <TableColumn fx:id="buildingNumber" prefWidth="111.0" text="Building Number" />
            <TableColumn fx:id="apartmentNumber" prefWidth="145.0" text="Apartement Number" />
            <TableColumn fx:id="tenant" prefWidth="233.0" text="Tenant" />
